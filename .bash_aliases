# c
alias c="reset"

# calc
alias calc="bc -l"

# df
alias df="df -Tha --total"

# nano
alias nano="nano --autoindent --softwrap"

# free
alias free="free -mt"

# header
alias header='curl -I'

# myip
alias myip="curl http://ipecho.net/plain; echo"

# psf
alias psf="ps aux | grep -v grep | grep -i -e VSZ -e"

# time
alias time="date + %d-%m-%Y_%T"

# wget
alias wget="wget -c --no-proxy"
